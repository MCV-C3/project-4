{
    "sweep_name": "scheduler_study",
    "baseline": {
        "dataset_path": "/ghome/group04/mcv/datasets/C3/2526/places_reduced",
        "batch_size": 256,
        "epochs": 40,
        "lr": 1e-3,
        "num_workers": 4,
        "wandb_project": "week2_sweeps",
        "wandb_entity": "c3-team4",
        "wandb_mode": "online",
        "optimizer_name": "adamw",
        "optimizer_params": {
            "weight_decay": 1e-4
        },
        "img_size": 64,
        "model_params": {
            "hidden_layers": [
                1024,
                512,
                256,
                128
            ],
            "dropout": 0.5, ##### change
