{
    "experiment_name": "week2_dense",
    "wandb_project": "week3-baselines",
    "wandb_entity": "c3-team4",
    "wandb_mode": "online",

    "evaluation_mode": "patch",
    "dataset_path": "/ghome/group04/mcv/datasets/C3/2425/MIT_large_train",
