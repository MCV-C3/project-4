        }
    },
    "variations": [
        {
            "name": "Scheduler_ReduceLROnPlateau",
            "scheduler_name": "reducelronplateau",
            "scheduler_params": {
                "mode": "min",
                "factor": 0.1,
                "patience": 5,
                "min_lr": 1e-6
            }
        },
        {
            "name": "CosineAnnealingLR",
            "scheduler_name": "cosineannealinglr",
            "scheduler_params": {
            "T_max": 40,
            "eta_min": 1e-6
            }
        },
        {
            "name": "StepLR",
            "scheduler_name": "steplr",
            "scheduler_params": {
            "step_size": 10,
            "gamma": 0.1
            }
        },
        {
            "name": "No_Scheduler",
            "scheduler_name": null,
            "scheduler_params": {}
        }
    ]
}